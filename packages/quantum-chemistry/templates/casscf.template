! OpenMolcas CASSCF calculation
! Generated by quantum-chemistry package
! Active space: ({n_active_electrons},{n_active_orbitals})
! System type: {point_group} symmetry

&GATEWAY
  COORD = {xyz_filename}
  BASIS = {basis_set}
  GROUP = {point_group}
END OF INPUT

&SEWARD
  CHOLESKY
END OF INPUT

&SCF
  CHARGE = {charge}
  SPIN = {unpaired_electrons}
  ITERATIONS = 100
  CONVERGENCE = 1.0E-08
END OF INPUT

&RASSCF
  NACTEL = {n_active_electrons} 0 0
  INACTIVE = {n_inactive}
  RAS2 = {n_active_orbitals}
  SPIN = {spin_multiplicity}
  SYMMETRY = 1
  CIROOT = {n_states} {n_states}
  MAXITER = {casscf_max_iter}
  THRS = {casscf_convergence}
  LEVSHFT = 0.0
{orbital_symmetry_section}
END OF INPUT

&GRID_IT
  NAME = ROOT.1.LUSCUS
  ALL
END OF INPUT